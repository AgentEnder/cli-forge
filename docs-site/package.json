{
  "name": "@flexi-bench/docs-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.4.0",
    "@docusaurus/preset-classic": "3.4.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/tsconfig": "3.4.0",
    "@docusaurus/types": "3.4.0",
    "docusaurus-plugin-typedoc": "^1.0.3",
    "remark-github": "^12.0.0",
    "typedoc": "^0.26.4",
    "typedoc-plugin-markdown": "^4.2.1",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "nx": {
    "name": "docs-site",
    "implicitDependencies": [
      "cli-forge",
      "parser"
    ],
    "targets": {
      "build": {
        "dependsOn": [
          "typecheck",
          "^build"
        ],
        "outputs": [
          "{projectRoot}/.docusaurus",
          "{projectRoot}/build",
          "{projectRoot}/docs/examples",
          "{projectRoot}/docs/api"
        ],
        "inputs": [
          "{projectRoot}/**/*",
          "{projectRoot}/docusaurus.config.js",
          "!{projectRoot}/src/**/*.d.ts",
          "!{projectRoot}/docs/examples",
          "!{projectRoot}/docs/api"
        ]
      },
      "deploy": {
        "dependsOn": [
          "build"
        ],
        "executor": "@nx-dotnet/nx-ghpages:deploy",
        "options": {
          "remote": "https://github.com/agentender/flexi-bench.git",
          "directory": "{projectRoot}/build",
          "baseBranch": "main"
        }
      }
    }
  }
}
